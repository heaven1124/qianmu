<script type="text/javascript">
function inadd(){
	 var incontent = $("#intext").val();
	 var inpid = $("#inpid").val();
	 var itemId=1185;
	 
	 $.ajax({
			url:ctx+"/addComment",
			data:{itemId:itemId,content:incontent,type:'item',pid:inpid},
			dataType:"json",
			type:"post",
			success:function(resp){
				if(resp.success){
					$("#pl").val(resp.msg);
					location.reload();
				}else{
					if("needlogin"==resp.status){
						$.fn.loginShow();
					}else{
						$.fn.notify("温馨提示",resp.msg);
					}
				}
			}
		});
}

var itemId = '1185' ;
var editLevel  = '';
if(itemId==''){
	itemId=2006;
}
var needlogin = '0';
	var needFill = '0';
 	var ads = '';  
var needpay = '0';
var vipnotice = '' ;
if(vl=='0'){
	
}
